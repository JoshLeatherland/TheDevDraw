  "compilerOptions": {
    "jsx": "react-jsx",
    "allowJs": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "target": "ESNext",
    "types": ["vite/client"]
  },
  "include": ["src", "../tdd-components/src/components/AppAppBar"],
  "exclude": ["node_modules"]
}